{
	"name": "Easy Login",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Quickly login to frequently used websites",
	"homepage_url": "http://google.com",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
	"permissions": ["<all_urls>", "storage", "unlimitedStorage", "topSites", "webNavigation"]
}