{
	"name": "Easy Login",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Quickly login to frequently used websites",
	"homepage_url": "http://google.com",
 	"background": {"persistent": false, "scripts": ["js/background.js", "js/sjcl.js"]},
	"browser_action": {
		"default_icon": "images/Cloud16.png"
	},
	"options_page": "index.html",
	"permissions": ["<all_urls>", "storage", "unlimitedStorage"],
	 "commands": {          
	 	  "toggle-feature": {
	 	  		"suggested_key": {
	 	  			"default": "Ctrl+9"
	 	  		},
	 	  		"description": "Easy Login Chrome extension launcher",
	 	  		"global": true
	 	  }
        }
}